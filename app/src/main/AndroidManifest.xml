        <activity
            android:name=".OneActivity">
        </activity>

        <activity
            android:name=".TwoActivity"
            android:label="@string/title_activity_main" >
        </activity>

        <activity android:name=".HintsActivity"
            android:theme="@style/Theme.D1NoTitleDim"
